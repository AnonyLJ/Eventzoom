{
  "name": "final-client-project-node",
  "version": "1.0.0",
  "description": "",
  "files": [
    "dist",
    ".env"
  ],
  "main": "lambda.js",
  "homepage": ".",
  "scripts": {
    "test": "jest --forceExit --detectOpenHandles --unhandled-rejections=strict",
    "start": "nodemon -r dotenv/config src/index.js --exec babel-node",
    "build": "node \"node_modules/@babel/cli/bin/babel.js\" src/ -d dist/src/ && node \"node_modules/@babel/cli/bin/babel.js\" config/ -d dist/config && cp -r src/views dist/src/views",
    "prod": "node -r dotenv/config dist/src/index.js dotenv_config_path=.env.prod",
    "lint": "eslint ./",
    "destroy-lambda": "aws lambda delete-function --function-name final-client-project-node --region eu-west-3 && aws iam delete-role-policy --role-name final-client-project-node-executor --policy-name log-writer --region eu-west-3 && aws iam delete-role --role-name final-client-project-node-executor --region eu-west-3 && rm claudia.json",
    "create-lambda": "npm run build && mv .env .env.dev && mv .env.serverless .env && npx claudia create --handler lambda.handler --deploy-proxy-api --region eu-west-3 --runtime nodejs10.x --timeout 10 && mv .env .env.serverless && mv .env.dev .env",
    "update-lambda": "npm run build && mv .env .env.dev && mv .env.serverless .env && npx claudia update && mv .env .env.serverless && mv .env.dev .env",
    "migrate": "npx db-migrate up",
    "seed": "npx db-migrate up:all",
    "create-seeder": "npx db-migrate create:seed",
    "create-migration": "npx db-migrate create",
    "create-email-scheduler": "node ./node_modules/claudia/bin/cmd.js add-scheduled-event --event sendMail.json --schedule \"rate(5 minutes)\" --name \"EmailScheduler\"",
    "create-reminder-scheduler": "node ./node_modules/claudia/bin/cmd.js add-scheduled-event --event sendReminders.json --schedule \"cron(0 9 * * ? *)\" --name \"ReminderScheduler\"",
    "destroy-email-scheduler": "node delete-scheduler.js EmailScheduler",
    "destroy-reminder-scheduler": "node delete-scheduler.js ReminderScheduler"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.cs.cf.ac.uk/c1734384/client-prorject-team-1"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "dotenv/config"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/runtime": "^7.7.2",
    "array-equal": "^1.0.0",
    "aws-sdk": "^2.619.0",
    "aws-serverless-express": "^3.3.6",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.6",
    "db-migrate-mongodb": "^1.5.0",
    "dotenv": "^8.2.0",
    "email-templates": "^7.0.4",
    "escape-string-regexp": "^2.0.0",
    "express": "^4.17.1",
    "ics": "^2.19.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.10",
    "mongoose-unique-validator": "^2.0.3",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.4",
    "passport": "^0.4.1",
    "passport-anonymous": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-saml": "^1.3.3",
    "pug": "^2.0.4",
    "redis": "^3.0.2",
    "regenerator-runtime": "^0.13.3",
    "string-similarity": "^4.0.1",
    "stripe": "^8.33.0",
    "twilio": "^3.40.0",
    "uuid": "^3.4.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/node": "^7.7.0",
    "babel-eslint": "^10.0.3",
    "claudia": "^5.12.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prefer-import": "0.0.1",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "supertest": "^4.0.2"
  }
}
